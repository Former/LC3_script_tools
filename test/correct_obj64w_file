#!/bin/bash
#input files
obj_file=$1
obj_correct_file=$2

# replace finish code to zero(nop) for stop programm after command "continue"
sed 's/\x00\x00\x00\x00\x00\x00\xC0\x41\xFF\xFF\xFF\xFF\xFF\xFF\xFF\x0F/\x00\x00\x00\x00\x00\x00\xC0\x41\x00\x00\x00\x00\x00\x00\x00\x00/g' $obj_file > $obj_correct_file


